{
    "version": 2,
    "builds": [
        {
            "src": "pages/api/**/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "*.html",
            "use": "@vercel/static"
        },
        {
            "src": "*.js",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/pages/api/$1"
        },
        {
            "src": "/app.js",
            "dest": "/app.js"
        },
        {
            "src": "/agent.js",
            "dest": "/agent.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}
