{
    "version": 2,
    "functions": {
        "api/*.js": {
            "memory": 1024,
            "maxDuration": 60
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}
